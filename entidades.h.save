#ifndef ENTIDADES_H_INCLUDED
#define ENTIDADES_H_INCLUDED

#include "dominios.h"

using namespace std;

//DEFINIÇÃO CONTA
class Conta {
    private:
            Cpf cpf;
            Nome nome;
            Senha senha;                   // Declarar cada atributo da classe.
    public:
            void setCpf(const Cpf&);
            void setNome(const Nome&);
            void setSenha(const Senha&);// Declarar método set para cada atributo.
            Cpf getCpf() const;
            Nome getNome() const;
            Senha getSenha() const;        // Declarar método get para cada atributo.
};

inline void Conta::setCpf(const Cpf &valor){
    this->cpf = valor;
}

inline Cpf Conta::getCpf() const{
    return cpf;
}

inline void Conta::setNome(const Nome &valor){
    this->nome = valor;
}

inline Nome Conta::getNome() const{
    return nome;
}

inline void Conta::setSenha(const Senha &valor){
    this->senha = valor;
}

inline Senha Conta::getSenha() const{
    return senha;
}

